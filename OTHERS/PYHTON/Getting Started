    print(username)

new_func()


def new_func1():
    secret_word = input("Guess the secret word: ")
    if secret_word == "Python":
        print("Correct! You guessed the secret word!")
    elif secret_word == "Java":
        print("Not quite, but close!")
    else:
        print("Try again!")

new_func1()


